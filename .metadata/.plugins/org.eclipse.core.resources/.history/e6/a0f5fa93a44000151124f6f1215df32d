package lab10.prob8;

import java.util.concurrent.Executor;
import java.util.concurrent.Executors;

public class Main {
	
	private static Executor exec = Executors.newCachedThreadPool();
	
	public static void main(String[] args) {
		
		Queue queue = new Queue();
		Runnable add = () -> {
			int i = 0;
			queue.add(i ++);
		};
		
		Runnable remove = () -> {
			queue.remove();
		};
		
		for(int i = 0; i < 10; ++i) {
			exec.execute(add);		
		}
		for(int i = 0; i < 10; ++i) {
			exec.execute(remove);		
		}
	}
}
